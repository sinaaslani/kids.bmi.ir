<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="microsoft.web.services3" type="Microsoft.Web.Services3.Configuration.WebServicesConfiguration, Microsoft.Web.Services3, Version=3.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"/>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="SSOWebSite.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <connectionStrings/>
  <system.web>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
    <compilation debug="true" targetFramework="4.0">   </compilation>
    <authentication mode="Windows"/>
    <customErrors mode="Off"/>

    <webServices>
      <soapExtensionImporterTypes>
        <add type="Microsoft.Web.Services3.Description.WseExtensionImporter, Microsoft.Web.Services3, Version=3.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"/>
      </soapExtensionImporterTypes>
    </webServices>
    
  </system.web>


  <appSettings>
    <add key="DefaultRedirect" value="http://127.0.0.1/"/>
    <add key="ExpirationMinutes" value="30"/>
    <add key="Domain" value="127.0.0.1"/>
    <add key="SSOUserName" value="morsali"/>
    <add key="SSOPassword" value="HyperServer"/>
    <add key="SSOWebServiceURL" value="http://127.0.0.1/BMISSOService/BMISSOService.asmx"/>
  </appSettings>
</configuration>